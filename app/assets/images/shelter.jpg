<script type="text/javascript">
function DownloadTimer() {
    var $countdown,
        $form, // Form used to change the countdown time
        incrementTime = 70,
        currentTime = 1500,
        onceTriggered = false,
        updateTimer = function() {
            $countdown.html((formatTime(currentTime) < 15) ? (formatTime(currentTime) + 1) : formatTime(currentTime));
            if (currentTime == 0) {
            	DownloadTimer.Timer.stop();
                timerComplete();
                //DownloadTimer.resetCountdown();
                return;
            }
            currentTime -= incrementTime / 10;
            if (currentTime < 0) currentTime = 0;
        },
        timerComplete = function() {
            $('#download-countdown').html('ダウンロードができるようになりました。');
			$("#timer-modal-download-area").fadeIn("slow");
			$('.timer-modal-img > img').css('opacity', '0.1');
			$('.timer-modal #download-countdown').css('color', '#333');
            $(".timer").fadeOut("slow");
            $('.dl_wrapper').css('height', '43px');
        	$(".download").fadeIn("slow");
        },
        formatTime = function(time) {
            var min = parseInt(time / 6000),
            sec = parseInt(time / 100) - (min * 60),
            hundredths = pad(time - (sec * 100) - (min * 6000), 2);
        	//return (min > 0 ? pad(min, 2) : "00") + ":" + pad(sec, 2) + ":" + hundredths;
        	return sec;
    	},
    	pad = function(number, length) {
    	    var str = '' + number;
    	    while (str.length < length) {str = '0' + str;}
    	    return str;
    	},
    	createDlButtons = function(){
    		 var dlButtons = $('.download').children().clone();
 			$('#timer-modal-download-area').append(dlButtons);
             $.each($('#timer-modal-download-area').children(), function(i, el){
                 var icon = '';
                 if(i == 0){
                     icon = 'jpeg-icon.png';
                 } else if(i == 1) {
                 	icon = 'png-icon.png';
                 } else if(i == 2) {
                 	icon = 'eps-icon.png';
                 } else if(i == 3) {
                 	icon = 'all-icon.png';
                 }

 				$(el).children().first().before('<img src="/images/' + icon + '" style="margin-bottom:-8px;max-width:60px" />');

 				var button = $(el);
				if(button.attr("data-remodal-target") == 'premium-illust-modal'){
					button.removeAttr("data-remodal-target");
					if(!button.hasClass('limit-large-dl')){
						button.on('click', function() {
						    $(this).addClass("on");
						    $(this).tooltip({
							position: {my: 'center bottom', at: 'center top'},
								items: '[data-format="ai"].on,[data-format="all"].on',
								tooltipClass: 'green-tooltip',
								content: function(){
									var html = '<div class="tooltip">';
									var size = $(this).attr('data-format');
									html += $('#prm-tooltip-msg-' + ((size=='ai') ? '2' : '4')).html();
									return html + '</div>';
								},
								close: function( event, ui ) {
								    ui.tooltip.hover(
									function () {
									    $(this).stop(true).fadeTo(400, 1);
									    //.fadeIn("slow"); // doesn't work because of stop()
									},
									function () {
									    $(this).fadeOut("400", function(){ $(this).remove(); })
									}
								    );
								    $(this).removeClass("on");
								  }
							});
							$(this).trigger('mouseenter');
						});
					}
				}
             });

    	};
    	this.init = function() {
    		onceTriggered = true;
            $countdown = $('#countdown');
            DownloadTimer.Timer = $.timer(updateTimer, incrementTime, true);

            if(!DownloadTimer.onceTriggered){
            	createDlButtons();
            }
        };
        this.ifOnceTriggered = function (){
			return onceTriggered;
        }
};

var dl = new DownloadTimer();
$(document).on('opened', '[data-remodal-id="timer-modal"]', function () {
	if(!dl.ifOnceTriggered()) {
		dl.init();
	}
});
</script>
