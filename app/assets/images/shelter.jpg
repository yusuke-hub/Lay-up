function addFav(data_id) {
	$("#jq-favo-message").remove();
	var sndRec = {data_id:data_id};
	$.ajax({
		url: "/ajax/addfav.php",
		type: "POST",
		dataType: "json",
		data: sndRec,
		success: function(rec) {

			if ( rec.status == "success" ) {
				$("#jq-add-bookmark-btn-img").attr("src", "/imgs/detail/bookmark_button2.jpg");
				$("#jq-add-bookmark-btn-img").parents("a").attr("onclick", "delFav('141987');");
				$('.header-bookmark-count').html(rec.cnt);
				$('.header-bookmark-count').effect('shake', {distance: "5"});
			}

			var top = $("#jq-add-bookmark-btn-img").offset().top - 40;
			var left = $("#jq-add-bookmark-btn-img").offset().left + 90;

			var html = '<div id="jq-favo-message" class="exec-favo-msg" style="display:none;z-index:200000;position:absolute;top:'+top+'px;left:'+left+'px;">';
			html += rec.mess;
			html + '</div>';

		    $("body").append(html);

			$("#jq-favo-message").fadeIn(500).delay(2000).fadeOut(2000);

		},
		error: function() {

		}
	});
}

function delFav(data_id) {
	$("#jq-favo-message").remove();
	var sndRec = {data_id:data_id};
	$.ajax({
		url: "/ajax/delfav.php",
		type: "POST",
		dataType: "json",
		data: sndRec,
		success: function(rec) {

			if ( rec.status == "success" ) {
				$("#jq-add-bookmark-btn-img").attr("src", "/imgs/detail/bookmark_button1.jpg");
				$("#jq-add-bookmark-btn-img").parents("a").attr("onclick", "addFav('141987');");
				$('.header-bookmark-count').html(rec.cnt);
				$('.header-bookmark-count').effect('shake', {distance: "5"});
			}

			var top = $("#jq-add-bookmark-btn-img").offset().top - 40;
			var left = $("#jq-add-bookmark-btn-img").offset().left + 90;

			var html = '<div id="jq-favo-message" class="exec-favo-msg" style="display:none;z-index:200000;position:absolute;top:'+top+'px;left:'+left+'px;">';
			html += rec.mess;
			html + '</div>';

		    $("body").append(html);

			$("#jq-favo-message").fadeIn(500).delay(2000).fadeOut(2000);

		},
		error: function(e) {

		}
	});
}

